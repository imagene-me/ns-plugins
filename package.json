    "@types/node": "14.14.33",
    "nativescript-ui-sidedrawer": "~10.0.1",
    "nativescript-vue": "~2.8.0",
    "nativescript-vue-template-compiler": "~2.8.0",
    "ng-packagr": "12.2.7",
    "rxjs": "6.6.7",
    "tslib": "^2.0.0",
    "typescript": "~4.3.0",
    "zone.js": "~0.11.4"
