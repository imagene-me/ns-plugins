{
	"version": 1,
	"projects": {
		"demo": {
			"root": "apps/demo/",
			"sourceRoot": "apps/demo/src",
			"projectType": "application",
			"prefix": "demo",
			"architect": {
				"ios": {
					"builder": "@nrwl/workspace:run-commands",
					"options": {
						"commands": ["ns debug ios --no-hmr --emulator --env.testing"],
						"cwd": "apps/demo",
						"parallel": false
					}
				},
				"android": {
					"builder": "@nrwl/workspace:run-commands",
					"options": {
						"commands": ["ns debug android --no-hmr --emulator --env.testing"],
						"cwd": "apps/demo",
						"parallel": false
					}
				},
				"clean": {
					"builder": "@nrwl/workspace:run-commands",
					"options": {
						"commands": ["ns clean", "npm i"],
						"cwd": "apps/demo",
						"parallel": false
					}
				}
			}
		},
		"demo-angular": {
			"root": "apps/demo-angular/",
			"sourceRoot": "apps/demo-angular/src",
			"projectType": "application",
			"prefix": "demo",
			"architect": {
				"ios": {
					"builder": "@nrwl/workspace:run-commands",
					"options": {
						"commands": ["ns debug ios --no-hmr --emulator --env.testing"],
						"cwd": "apps/demo-angular",
						"parallel": false
					}
				},
				"android": {
					"builder": "@nrwl/workspace:run-commands",
					"options": {
						"commands": ["ns debug android --no-hmr --emulator --env.testing"],
						"cwd": "apps/demo-angular",
						"parallel": false
					}
				},
				"clean": {
					"builder": "@nrwl/workspace:run-commands",
					"options": {
						"commands": ["ns clean", "npm i"],
						"cwd": "apps/demo-angular",
						"parallel": false
					}
				}
			}
		},
		"all": {
			"root": "packages",
			"projectType": "library",
			"architect": {
				"build": {
					"builder": "@nrwl/workspace:run-commands",
					"outputs": ["dist/packages"],
					"options": {
						"commands": ["nx run nativescript-highcharts:build.all"],
						"parallel": false
					}
				},
				"focus": {
					"builder": "@nrwl/workspace:run-commands",
					"outputs": ["dist/packages"],
					"options": {
						"commands": ["nx g @nativescript/plugin-tools:focus-packages"],
						"parallel": false
					}
				}
			}
		},
		"nativescript-highcharts": {
			"root": "packages/nativescript-highcharts",
			"sourceRoot": "packages/nativescript-highcharts",
			"projectType": "library",
			"schematics": {},
			"architect": {
				"build": {
					"builder": "@nrwl/node:package",
					"options": {
						"outputPath": "dist/packages/nativescript-highcharts",
						"tsConfig": "packages/nativescript-highcharts/tsconfig.json",
						"packageJson": "packages/nativescript-highcharts/package.json",
						"main": "packages/nativescript-highcharts/index.ts",
						"assets": [
							"packages/nativescript-highcharts/*.md",
							"packages/nativescript-highcharts/index.d.ts",
							"packages/nativescript-highcharts/common.xml",
							"LICENSE",
							{
								"glob": "**/*",
								"input": "packages/nativescript-highcharts/platforms/",
								"output": "./platforms/"
							}
						]
					}
				},
				"build.all": {
					"builder": "@nrwl/workspace:run-commands",
					"outputs": ["dist/packages"],
					"options": {
						"commands": ["nx run nativescript-highcharts:build", "node tools/scripts/build-finish.ts nativescript-highcharts"],
						"parallel": false
					}
				},
				"focus": {
					"builder": "@nrwl/workspace:run-commands",
					"outputs": ["dist/packages"],
					"options": {
						"commands": ["nx g @nativescript/plugin-tools:focus-packages nativescript-highcharts"],
						"parallel": false
					}
				}
			}
		},
		"nativescript-toast": {
			"root": "packages/nativescript-toast",
			"sourceRoot": "packages/nativescript-toast",
			"projectType": "library",
			"schematics": {},
			"architect": {
				"build": {
					"builder": "@nrwl/node:package",
					"options": {
						"outputPath": "dist/packages/nativescript-toast",
						"tsConfig": "packages/nativescript-toast/tsconfig.json",
						"packageJson": "packages/nativescript-toast/package.json",
						"main": "packages/nativescript-toast/toasty",
						"typings": "packages/nativescript-toast/index.d.ts",
						"assets": [
							"packages/nativescript-toast/*.md",
							"packages/nativescript-toast/index.d.ts",
							"packages/nativescript-toast/common.xml",
							"LICENSE",
							{
								"glob": "**/*",
								"input": "packages/nativescript-toast/platforms/",
								"output": "./platforms/"
							}
						]
					}
				},
				"build.all": {
					"builder": "@nrwl/workspace:run-commands",
					"outputs": ["dist/packages"],
					"options": {
						"commands": ["nx run nativescript-toast:build", "node tools/scripts/build-finish.ts nativescript-toast"],
						"parallel": false
					}
				},
				"focus": {
					"builder": "@nrwl/workspace:run-commands",
					"outputs": ["dist/packages"],
					"options": {
						"commands": ["nx g @nativescript/plugin-tools:focus-packages nativescript-toast"],
						"parallel": false
					}
				}
			}
		}
	},
	"cli": {
		"defaultCollection": "@nrwl/workspace"
	}
}
