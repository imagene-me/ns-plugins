{
  "version": 1,
  "projects": {
    "demo-angular": {
      "root": "apps/demo-angular/",
      "sourceRoot": "apps/demo-angular/src",
      "projectType": "application",
      "prefix": "demo",
      "architect": {
        "ios": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "ns debug ios --no-hmr --emulator --env.testing"
            ],
            "cwd": "apps/demo-angular",
            "parallel": false
          }
        },
        "android": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "ns debug android --no-hmr --emulator --env.testing"
            ],
            "cwd": "apps/demo-angular",
            "parallel": false
          }
        },
        "clean": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "ns clean",
              "npm i"
            ],
            "cwd": "apps/demo-angular",
            "parallel": false
          }
        }
      }
    },
    "all": {
      "root": "",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": [
            "dist/packages"
          ],
          "options": {
            "commands": [
              "nx run nativescript-highcharts:build.all",
              "nx run nativescript-google-login:build.all",
              "nx run nativescript-toast:build.all",
              "nx run nativescript-shadow:build.all",
              "nx run nativescript-ox-button:build.all"
            ],
            "parallel": false
          }
        },
        "focus": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": [
            "dist/packages"
          ],
          "options": {
            "commands": [
              "nx g @nativescript/plugin-tools:focus-packages"
            ],
            "parallel": false
          }
        }
      },
      "sourceRoot": ""
    },
    "nativescript-highcharts": {
      "root": "packages/nativescript-highcharts",
      "sourceRoot": "packages/nativescript-highcharts",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/nativescript-highcharts",
            "tsConfig": "packages/nativescript-highcharts/tsconfig.json",
            "packageJson": "packages/nativescript-highcharts/package.json",
            "main": "packages/nativescript-highcharts/index.ts",
            "assets": [
              "packages/nativescript-highcharts/*.md",
              "packages/nativescript-highcharts/index.d.ts",
              "packages/nativescript-highcharts/common.xml",
              "LICENSE",
              {
                "glob": "**/*",
                "input": "packages/nativescript-highcharts/platforms/",
                "output": "./platforms/"
              }
            ]
          }
        },
        "build.all": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": [
            "dist/packages"
          ],
          "options": {
            "commands": [
              "nx run nativescript-highcharts:build",
              "node tools/scripts/build-finish.ts nativescript-highcharts"
            ],
            "parallel": false
          }
        },
        "focus": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": [
            "dist/packages"
          ],
          "options": {
            "commands": [
              "nx g @nativescript/plugin-tools:focus-packages nativescript-highcharts"
            ],
            "parallel": false
          }
        }
      }
    },
